snippet table
	\begin{table}
		\begin{center}
	       	\caption{${1}}
			\begin{tabular}[htbp]{${2:l}}
	\toprule

	\midrule

	\bottomrule
			\label{Table:${3}}
			\end{tabular}
		\end{center}
	\end{table}
